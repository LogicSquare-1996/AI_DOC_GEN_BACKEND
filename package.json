{
  "name": "express-skeleton-3",
  "version": "1.0.0",
  "description": "A skeleton application for quickly starting an Express.js project.",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "node ./bin/www",
    "serve": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www",
    "lint": "eslint --ext .js,.jsx .",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=development nyc --reporter=lcov --reporter=text mocha --timeout 10000 --exit"
  },
  "keywords": [
    "express",
    "skeleton",
    "starter",
    "application",
    "nodejs"
  ],
  "author": "Kiran Debnath <kiran@logic-square.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LogicSquare-1996/Node-JS-Scalaton.git"
  },
  "dependencies": {
    "agenda": "^3.1.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "email-templates": "^8.0.10",
    "express": "~4.16.1",
    "express-jwt": "^6.1.1",
    "helmet": "^5.0.2",
    "http-errors": "~1.6.3",
    "mongodb-memory-server": "^8.4.1",
    "mongoose": "^5.10.15",
    "morgan": "~1.9.1",
    "randomstring": "^1.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "eslintIgnore": [
    "public/",
    "bin/"
  ],
  "devDependencies": {
    "ava": "^4.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^6.6.6",
    "husky": "^4.3.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "chai": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:quiet"
    }
  }
}
